{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/config-file.v1.json",
  "name": "frontend",
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview"
  },
  "imports": {
    "@tanstack/react-query": "npm:@tanstack/react-query@^5.90.5",
    "axios": "npm:axios@^1.13.1",
    "preact": "npm:preact@^10.26.9",
    "preact-iso": "npm:preact-iso@^2.10.0",

    "@preact/preset-vite": "npm:@preact/preset-vite@^2.10.2",
    "eslint": "npm:eslint@^9.35.0",
    "eslint-config-preact": "npm:eslint-config-preact@^2.0.0",
    "rollup": "npm:rollup@^4.53.3",
    "typescript": "npm:typescript@^5.9.2",
    "vite": "npm:vite@^7.0.4"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "exports": {}
}
